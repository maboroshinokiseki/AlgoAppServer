@page
@model AlgoApp.Pages.Admin.Messages.QuestionReportMessageModel
@{
    ViewData["Title"] = "QuestionReportMessage";
}


<div class="form-group row">
    <label asp-for="MessageContentDetail.QuestionContent" class="col-form-label col-sm-2"></label>
    <div class="col-sm-10">
        <a asp-page="/Admin/Questions/Edit" asp-route-chapterid="@Model.MessageContentDetail.ChapterId" asp-route-id="@Model.MessageContentDetail.QuestionId">@Model.MessageContentDetail.QuestionContent</a>
    </div>
</div>
<div class="form-group row">
    <label asp-for="MessageContentDetail.Content" class="col-form-label col-sm-2"></label>
    <div class="col-sm-10">
        <textarea asp-for="MessageContentDetail.Content" class="form-control"></textarea>
    </div>
</div>

<a href="." class="btn btn-primary">返回</a>
